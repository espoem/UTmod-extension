{
  "manifest_version": 2,
  "name": "UTmod",
  "version": "0.4.0",

  "description": "Extension to collect tools for Utopian reviewers to one place.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "UTmod",
    "default_icon": {
      "128": "img/UT-128px.png",
      "64": "img/UT-64px.png",
      "32": "img/UT-32px.png"
    }
  },
  "icons": {
    "128": "img/UT-128px.png",
    "64": "img/UT-64px.png",
    "32": "img/UT-32px.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
